		<dc:title id="title">Germinie Lacerteux</dc:title>
		<meta property="file-as" refines="#title">Germinie Lacerteux</meta>
		<dc:subject id="subject-1">Working class women -- France -- Fiction</dc:subject>
		<dc:subject id="subject-2">Women -- France -- History -- 19th century -- Fiction</dc:subject>
		<meta property="authority" refines="#subject-1">LCSH</meta>
		<meta property="term" refines="#subject-1">Unknown</meta>
		<meta property="authority" refines="#subject-2">LCSH</meta>
